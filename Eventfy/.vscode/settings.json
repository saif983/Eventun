{
  "css.validate": false,
  "scss.validate": false,
  "scss.lint.unknownAtRules": "ignore",
  "less.validate": false,
  "css.customData": [
    ".vscode/css_custom_data.json"
  ],
  "files.associations": {
    "*.scss": "scss"
  },
  "emmet.includeLanguages": {
    "scss": "css"
  },
  "tailwindCSS.includeLanguages": {
    "scss": "scss"
  },
  "tailwindCSS.experimental.classRegex": [
    [
      "@apply\\s+([^;}]+)",
      "([^;}]+)"
    ],
    [
      "classList\\.add\\(([^)]*)\\)",
      "([^)]*)"
    ]
  ]
}